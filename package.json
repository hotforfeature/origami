{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@codebakery/origami",
  "version": "3.1.2",
  "description": "Angular + Polymer",
  "keywords": [
    "Angular",
    "Polymer",
    "webcomponents",
    "elements"
  ],
  "contributors": [
    "Liz Mitchell <asyncliz@gmail.com>",
    "Andrew Mitchell <abmitchell77@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hotforfeature/origami.git"
  },
  "homepage": "https://github.com/hotforfeature/origami#readme",
  "bugs": {
    "url": "https://github.com/hotforfeature/origami/issues"
  },
  "bin": {
    "origami": "./bin/origami.js"
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "postbuild": "tsc -p bin/",
    "test": "karma start",
    "test:ci": "SAUCE=true karma start --single-run",
    "lint": "prettier --write \"*.{js,ts,json,md}\" \"{bin,forms,polyfills,shadycss,src,templates,util}/**/*\"",
    "precommit": "lint-staged",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "flatModuleFile": "origami"
    },
    "whitelistedNonPeerDependencies": [
      "chalk",
      "node-glob",
      "parse5",
      "progress",
      "yargs"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "node-glob": "^1.2.0",
    "parse5": "^5.0.0",
    "progress": "^2.0.0",
    "yargs": "^12.0.1"
  },
  "peerDependencies": {
    "@angular/core": "^5.0.0-rc.0 || ^5.0.0 || ^6.0.0-rc.0 || ^6.0.0 || ^7.0.0-rc.0 || ^7.0.0 || ^8.0.0-rc.0 || ^8.0.0",
    "@angular/common": "^5.0.0-rc.0 || ^5.0.0 || ^6.0.0-rc.0 || ^6.0.0 || ^7.0.0-rc.0 || ^7.0.0 || ^8.0.0-rc.0 || ^8.0.0",
    "@angular/forms": "^5.0.0-rc.0 || ^5.0.0 || ^6.0.0-rc.0 || ^6.0.0 || ^7.0.0-rc.0 || ^7.0.0 || ^8.0.0-rc.0 || ^8.0.0",
    "@angular/router": "^5.0.0-rc.0 || ^5.0.0 || ^6.0.0-rc.0 || ^6.0.0 || ^7.0.0-rc.0 || ^7.0.0 || ^8.0.0-rc.0 || ^8.0.0",
    "@polymer/polymer": "^3.0.5",
    "@webcomponents/webcomponentsjs": "^2.0.4",
    "tslib": "^1.7.0"
  },
  "devDependencies": {
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@types/glob": "^5.0.35",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.5.5",
    "@types/parse5": "^5.0.0",
    "@types/progress": "^2.0.1",
    "@types/yargs": "^11.1.1",
    "@vaadin/vaadin-checkbox": "^2.2.6",
    "@vaadin/vaadin-combo-box": "^4.2.3",
    "@vaadin/vaadin-date-picker": "^3.3.2",
    "@webcomponents/webcomponentsjs": "^2.2.6",
    "core-js": "^2.5.7",
    "cpr": "^3.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^2.0.2",
    "karma-typescript": "^4.0.0-beta.3",
    "karma-typescript-es6-transform": "^4.0.0-beta.3",
    "lint-staged": "^7.2.0",
    "ng-packagr": "^5.3.0",
    "prettier": "^1.16.3",
    "rxjs": "^6.5.2",
    "standard-version": "^4.4.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5",
    "xhr-mock": "^2.4.1",
    "zone.js": "^0.9.1"
  },
  "engine": {
    "node": ">=8.0.0"
  }
}
