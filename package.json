{
  "name": "@codebakery/origami",
  "version": "1.1.2",
  "description": "Angular + Polymer",
  "keywords": [
    "Angular",
    "Polymer"
  ],
  "author": "Andrew Mitchell <abmitchell77@gmail.com>",
  "license": "MIT",
  "module": "lib/origami.js",
  "types": "lib/origami.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hotforfeature/origami.git"
  },
  "homepage": "https://github.com/hotforfeature/origami#readme",
  "bugs": {
    "url": "https://github.com/hotforfeature/origami/issues"
  },
  "scripts": {
    "clean": "rimraf lib/ coverage/ test/build/",
    "lint": "tslint --project tsconfig.lint.json",
    "bower": "bower install",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "pretest:es5": "cd test && polymer build",
    "test:es5": "ES5=true karma start",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "commit": "git-cz",
    "prebuild": "npm run clean",
    "build": "ngc -p tsconfig.json",
    "release": "standard-version",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/platform-browser": "^4.0.2"
  },
  "devDependencies": {
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.12",
    "awesome-typescript-loader": "^3.1.2",
    "bower": "^1.8.0",
    "codelyzer": "^2.1.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.3",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "polymer-cli": "^0.18.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.0.1",
    "source-map-loader": "^0.2.1",
    "standard-version": "^4.0.0",
    "tslint": "^4.5.1",
    "typescript": "~2.1.6",
    "validate-commit-msg": "^2.11.2",
    "webpack": "^2.4.1",
    "zone.js": "^0.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
